{
  "targetArn": "arn:aws:iot:eu-central-1:533267070201:thing/EdgeLLMDemo",
  "deploymentName": "Minimal-Clean-Deployment",
  "components": {
    "aws.greengrass.Nucleus": {
      "componentVersion": "2.15.0"
    },
    "aws.greengrass.Cli": {
      "componentVersion": "2.15.0"
    }
  },
  "deploymentPolicies": {
    "failureHandlingPolicy": "ROLLBACK",
    "componentUpdatePolicy": {
      "timeoutInSeconds": 900,
      "action": "NOTIFY_COMPONENTS"
    }
  }
}
